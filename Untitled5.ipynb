    "string = \"suraj\"\n",
    "index = -1\n",
    "sep = \"\"\n",
    "for i in string:\n",
    "    if string.count(i) == 1:\n",
    "        sep += i\n",
    "        break\n",
    "    else:\n",
    "        index += 1\n",
    "if index == 1:\n",
    "    print(\"Either all characters are repeating or string is empty\")\n",
    "else:\n",
    "    print(\"First non-repeating character is\", sep)"
